<MOVE Order="" Id="CCWRadius">
  <NAME>CCW Radius</NAME>
  <ICON>010.ico</ICON>
  <HELPFORM>CCWRad.htm</HELPFORM>
  <CODE>
G03 G91 Y[-1*{Y_TARGET}] Z[{Z_TARGET}] J{J_VECTOR} K[{K_VECTOR}] F{RADIUS_VELOCITY}	(CCW Radius)
  </CODE>
  <VARIABLES>
    <VAR no="1" id="A">
      <ID>RADIUS_VALUE</ID>
      <DESCRIPTION>Radius Value</DESCRIPTION>
      <ALT_DESC>Radius Value</ALT_DESC>
      <VALUE>00.00000</VALUE>
      <CONTAINER>2</CONTAINER>
      <CONSTANT>0</CONSTANT>
      <VALUEBOXPROPERTIES>
	<VALUECOLORBACK>65280</VALUECOLORBACK>
	<VALUEBOXTYPE>0</VALUEBOXTYPE>
	<LEDFLASH>0</LEDFLASH>
      </VALUEBOXPROPERTIES>
    </VAR>
    <VAR no="2" id="B">
      <ID>START_PERCENT</ID>
      <DESCRIPTION>Start Percent</DESCRIPTION>
      <ALT_DESC>Start Percent</ALT_DESC>
      <VALUE>00.00000</VALUE>
      <CONTAINER>2</CONTAINER>
      <CONSTANT>1</CONSTANT>
      <VALUEBOXPROPERTIES>
	<VALUECOLORBACK>16777215</VALUECOLORBACK>
	<VALUEBOXTYPE>0</VALUEBOXTYPE>
	<LEDFLASH>0</LEDFLASH>
	<IGNOREMETRIC>1</IGNOREMETRIC>
      </VALUEBOXPROPERTIES>
    </VAR>
    <VAR no="3" id="C">
      <ID>END_PERCENT</ID>
      <DESCRIPTION>End Percent</DESCRIPTION>
      <ALT_DESC>End Percent</ALT_DESC>
      <VALUE>00.50000</VALUE>
      <CONTAINER>3</CONTAINER>
      <CONSTANT>1</CONSTANT>
      <VALUEBOXPROPERTIES>
	<VALUECOLORBACK>16777215</VALUECOLORBACK>
	<VALUEBOXTYPE>0</VALUEBOXTYPE>
	<LEDFLASH>0</LEDFLASH>
	<IGNOREMETRIC>1</IGNOREMETRIC>
      </VALUEBOXPROPERTIES>
    </VAR>
    <VAR no="4" id="D">
      <ID>RADIUS_VELOCITY</ID>
      <DESCRIPTION>Radius Velocity</DESCRIPTION>
      <ALT_DESC>Radius Velocity</ALT_DESC>
      <VALUE>0.10000</VALUE>
      <CONTAINER>5</CONTAINER>
      <CONSTANT>0</CONSTANT>
      <VALUEBOXPROPERTIES>
	<VALUECOLORBACK>65280</VALUECOLORBACK>
	<VALUEBOXTYPE>0</VALUEBOXTYPE>
	<LEDFLASH>0</LEDFLASH>
	<ISVELOCITY>1</ISVELOCITY>
      </VALUEBOXPROPERTIES>
    </VAR>
    <VBS>
      <ID>START_ANGLE</ID>
      <SCRIPT>{START_PERCENT}*180</SCRIPT>
    </VBS>
    <VBS>
      <ID>END_ANGLE</ID>
      <SCRIPT>{END_PERCENT}*180</SCRIPT>
    </VBS>
    <VBS>
      <ID>J_VECTOR</ID>
      <SCRIPT>round({RADIUS_VALUE}*sin({START_ANGLE} * 3.1415926535897932384626433832795 / 180),5)</SCRIPT>
    </VBS>
    <VBS>
      <ID>K_VECTOR</ID>
      <SCRIPT>round({RADIUS_VALUE}*cos({START_ANGLE} * 3.1415926535897932384626433832795 / 180),5)</SCRIPT>
    </VBS>
    <VBS>
      <ID>Y_TARGET</ID>
      <SCRIPT>round({RADIUS_VALUE}*sin({END_ANGLE} * 3.1415926535897932384626433832795 / 180) - {J_VECTOR},5)</SCRIPT>
    </VBS>
    <VBS>
      <ID>Z_TARGET</ID>
      <SCRIPT>round({K_VECTOR} - {RADIUS_VALUE}*cos({END_ANGLE} * 3.1415926535897932384626433832795 / 180),5)</SCRIPT>
    </VBS>
  </VARIABLES>
  <HELP_TEXT>This move is used to blah blah blah</HELP_TEXT>
  <HELP_VIDEO>NAME_OF_VIDEO.AVI</HELP_VIDEO>
  <QUICK_TIP_VIDEO>ccw radius.avi</QUICK_TIP_VIDEO>
  <MOVEINDEX>0</MOVEINDEX>
  <MOVESTATUS>Natural</MOVESTATUS>
</MOVE>
